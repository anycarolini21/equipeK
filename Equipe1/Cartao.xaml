<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Equipe1.Cartao">

    <StackLayout>
        <Label Text="Cartão" FontSize="Title" HorizontalTextAlignment="Center" />
        <Label Text="Crédito ou Débito?" HorizontalTextAlignment="Center" />
        <Button Text="Crédito" Clicked="OnCreditoClicked" />
        <Button Text="Débito" Clicked="OnDebitoClicked" />
    </StackLayout>

</ContentPage>
